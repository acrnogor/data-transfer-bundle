{
    "name": "kuborgh/data-transfer-bundle",
    "type": "library",
    "description": "Console Commands to easily transfer live data (database + files) to another environment",
    "keywords": ["library","command","database","transfer","files"],
    "homepage": "https://github.com/kuborgh/data-transfer-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Hannes Giesenow",
            "email": "hgiesenow@kuborgh.de"
        }
    ],
    "require": {
        "php": ">=5.3.2",
		"symfony/console": "*",
		"symfony/process": "*"
    },
    "require-dev": {
        "symfony/console": "*"
    },
    "autoload": {
        "psr-0": { "Kuborgh\\DataTransferBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Kuborgh/DataTransferBundle"
}
